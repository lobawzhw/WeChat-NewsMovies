<template name="starsTemplate">
  <view class="stars-container">
    <view class="stars">
      <block wx:for='{{star}}' wx:for-item='star'>
        <image src="{{star ? '/images/icon/star.png' : '/images/icon/none-star.png'}}"></image>
      </block>
    </view>
    <text class="star-score">{{score}}</text>
  </view>
</template>